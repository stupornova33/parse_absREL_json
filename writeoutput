setwd <- getwd()
options("scipen"=0, "digits"=6)
file1 <- paste(readLines("genename"), collapse=" ")
file2 <- paste(readLines("branchid"), collapse=" ")

genedt <- data.table::data.table(gene = character(1L), branch = character(1L))
genedt$gene[1] <- file1
genedt$branch[1] <- file2
print(genedt)
write.table(genedt, file = "output", append = TRUE, row.names = FALSE, col.names = FALSE, quote = FALSE)
